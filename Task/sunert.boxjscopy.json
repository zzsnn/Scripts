      "repo": "https://github.com/zzsnn/Scripts/blob/master/Task/youth.js", 
      "id": "youth", 
      "script": "https://raw.githubusercontent.com/zzsnn/Scripts/master/Task/youth.js", 
      "settings": [
        {
          "id": "zqcard", 
          "val": "false", 
          "name": "打卡赚钱", 
          "type": "boolean", 
          "desc": "每日打卡报名及早起打卡"
        }, 
        {
          "id": "zqtime", 
          "val": "05", 
          "name": "打卡时间", 
          "type": "number", 
          "desc": "每日早起打卡时间"
